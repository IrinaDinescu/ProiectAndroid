<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".SoldCurentActivity"
    >

    <ImageView
        android:id="@+id/homeBanking"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="70dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/homebanking"
        android:contentDescription="@string/HomeBanking" />
    <ImageView
        android:id="@+id/bani"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="150dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="125dp"
        android:src="@drawable/bani"
        android:contentDescription="@string/bani" />
    <TextView
        android:id="@+id/tv_sold"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:hint="Cont Curent"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:textAlignment="center"
        android:textSize="30sp"
        />
    <ImageView
        android:id="@+id/sac"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/sac"
        android:contentDescription="@string/sac" />
    <Button
        android:id="@+id/bt_plati"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:text="@string/Transferabani"
        android:layout_marginLeft="70dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="310dp"
        android:background="#FF8C00"
        android:textSize="15sp"
        android:gravity="center"
        android:textAllCaps="false"
        />
    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="210dp"
        android:hint="Ai in conturi"
        android:layout_marginLeft="10dp"
        />

    <ImageView
        android:id="@+id/economii"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="400dp"
        android:src="@drawable/economii"
        android:contentDescription="@string/economii" />
    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="410dp"
        android:hint="Cont Economii"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/contEconomii"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="410dp"
        android:hint=""
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/tvcontEURO"
        android:layout_width="115dp"
        android:layout_height="50dp"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="210dp"
        android:layout_marginRight="50dp"
        android:hint="@string/ContCurentLei"
        android:textSize="20sp"
        android:text=""
        />
    <TextView
        android:id="@+id/euro"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="330dp"
        android:layout_marginRight="40dp"
        android:hint="@string/ContCurentLei"
        android:textSize="20sp"
        android:text="EUR"
        />

    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="450dp"
        android:text="@string/push_alerts"
        android:layout_marginLeft="50dp"
        />

    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="490dp"
        android:text="@string/banca_pay"
        android:layout_marginLeft="50dp"
        />

    <Switch
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="440dp"
        android:layout_marginLeft="200dp"
        />

    <Switch
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="480dp"
        android:layout_marginLeft="200dp"
        />
    <TextView
        android:id="@+id/tvcontLEI"
        android:layout_width="115dp"
        android:layout_height="50dp"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="50dp"
        android:hint="@string/ContCurentLei"
        android:textSize="20sp"
        android:text=""
        />
    <TextView

        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="140dp"
        android:layout_marginRight="50dp"
        android:hint="@string/ContCurentLei"
        android:textSize="20sp"
        android:text="LEI"
        />

</RelativeLayout>